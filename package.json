{
  "name": "baluni",
  "version": "2.4.6",
  "description": "Uniswap v3 rebalance and forecast CLI tool",
  "files": [
    "dist"
  ],
  "scripts": {
    "ts-node": "ts-node",
    "build": "npx tsc",
    "rebalance": "npx ts-node src/core/strategies/rebalance/rebalance.ts",
    "rebalanceV2": "npx ts-node src/core/strategies/rebalanceV2/rebalance.ts",
    "invest": "npx ts-node src/core/strategies/investor/investor.ts",
    "dca": "npx ts-node src/core/dca.ts",
    "pumpOrDump": "npx ts-node src/strategies/pumpOrDump/pumpOrDump.ts",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "check-types": "tsc --noEmit --incremental",
    "dev": "tsc --watch",
    "start": "npx ts-node src/api/server.ts",
    "lint": "eslint --ext .js,.ts"
  },
  "keywords": [
    "defi",
    "uniswap",
    "rebalance",
    "erc20",
    "cli",
    "evm"
  ],
  "author": "scobru",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@types/node": "^20.10.5",
    "@uniswap/sdk-core": "^4.0.10",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/smart-order-router": "3.19.3",
    "asciichart": "^1.5.25",
    "axios": "^1.6.5",
    "chakl": "^0.0.1-security",
    "cli-chart": "^0.3.1",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^5.7.2",
    "express": "^4.18.3",
    "jsbi": "3.2.5",
    "prettier": "^2.8.4",
    "trading-indicator": "^2.0.4",
    "@uniswap/permit2-sdk": "^1.2.0",
    "@uniswap/router-sdk": "1.7.1",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "ethereum-abi-types-generator": "^1.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scobru/baluni.git"
  },
  "bugs": {
    "url": "https://github.com/scobru/baluni/issues"
  },
  "homepage": "https://github.com/scobru/baluni#readme"
